@article{10.1046/j.1365-246X.2002.01758.x,
    author = {Lowman, Julian},
    title = "{Mantle Convection in the Earth and Planets}",
    journal = {Geophysical Journal International},
    volume = {150},
    number = {3},
    pages = {827-827},
    year = {2002},
    month = {09},
    abstract = "{In its infancy, the plate tectonic paradigm reconciled the evidence for continental drift within a more widely encompassing theory that identified that the continents change position as a consequence of the entire ocean floor being continually recycled. For the geoscientist studying the cooling of the Earth, plate tectonics is similarly a specific manifestation of the evolution of a more fundamental process, mantle convection. Mantle convection is the primary means by which heat is removed from the Earth's mantle and core. Consequently, mantle convection is the driving force responsible for plate tectonics, volcanism, earthquakes and orogenesis. It influences the Earth's topography and gravitational field, the geodynamo, the formation of mineral and hydrocarbon resources and the planet's climatic system, including cycles of glaciation and biological evolution. Accordingly, the study of each of these disciplines reveals some expression of mantle convection and, conversely, our evolving understanding of mantle convection can affect conventional views of a wide range of fields in the Earth Sciences. It is the influence of mantle convection on so many fields of geophysics that will make the thorough examination of the subject in Mantle Convection in the Earths and Planets of considerable interest to all Earth scientists.}",
    issn = {0956-540X},
    doi = {10.1046/j.1365-246X.2002.01758.x},
    url = {https://doi.org/10.1046/j.1365-246X.2002.01758.x},
    eprint = {http://oup.prod.sis.lan/gji/article-pdf/150/3/827/1694847/150-3-827.pdf},
}


@Article{Bajgain2015,
author={Bajgain, Suraj
and Ghosh, Dipta B.
