@article {Scott2004,
	author = {Scott, Henry P. and Hemley, Russell J. and Mao, Ho-kwang and Herschbach, Dudley R. and Fried, Laurence E. and Howard, W. Michael and Bastea, Sorin},
	title = {Generation of methane in the Earth{\textquoteright}s mantle: In situ high pressure{\textendash}temperature measurements of carbonate reduction},
